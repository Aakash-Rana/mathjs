{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES6",
    "moduleResolution": "node",
    "lib": ["ES6", "DOM"],
    "module": "ESNEXT",
    "noEmit": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": false,
    "strictFunctionTypes": true,
    "forceConsistentCasingInFileNames": true
  }
}
